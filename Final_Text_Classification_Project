t seaborn as sns\n    sns.heatmap(cm, annot=True, fmt='d', cmap='Blues', xticklabels=labels, yticklabels=labels, ax=ax)\n    ax.set_title(title)\n    ax.set_xlabel('Pred')\n    ax.set_ylabel('True')\n    plt.show()\n\ndef eval_sklearn(model, vectorizer, Xtr_texts, Xva_texts, ytr, yva, model_name, vect_name):\n    Xtr = vectorizer.fit_transform(Xtr_texts)\n    Xva = vectorizer.transform(Xva_texts)\n    model.fit(Xtr, ytr)\n    y_pred = model.predict(Xva)\n    acc = accuracy_score(yva, y_pred)\n    prec_w, rec_w, f1_w, _ = precision_recall_fscore_support(yva, y_pred, average='weighted', zero_division=0)\n    print(f\"{model_name} + {vect_name}: acc={acc:.4f} f1_w={f1_w:.4f}\")\n    print(classification_report(yva, y_pred, target_names=list(le.classes_)))\n    plot_confusion(yva, y_pred, list(le.classes_), f\"{model_name} ({vect_name})\")\n    return {\n        'model': model_name,\n        'vectorizer': vect_name,\n        'accuracy': round(acc,4),\n        'precision': round(prec_w,4),\n        'recall': round(rec_w,4),\n        'f1': round(f1_w,4),\n        'timestamp': time.strftime('%Y-%m-%d %H:%M:%S')\n    }\n\nresults_df = pd.DataFrame(columns=['model','vectorizer','accuracy','precision','recall','f1','timestamp'])\n\n# SVM + TF-IDF(1-4)\ntfidf_14 = TfidfVectorizer(ngram_range=(1,4), max_features=2_000_000, sublinear_tf=True)\nsvm = LinearSVC()\nrow = eval_sklearn(svm, tfidf_14, X_train_texts, X_val_texts, y_train, y_val, 'SVM', 'TF-IDF (1-4gram)')\nresults_df = pd.concat([results_df, pd.DataFrame([row])], ignore_index=True)\n\n# Logistic Regression + BoW(1-4)\nbow_14 = CountVectorizer(ngram_range=(1,4), max_features=2_000_000)\nlr = LogisticRegression(max_iter=200)\nrow = eval_sklearn(lr, bow_14, X_train_texts, X_val_texts, y_train, y_val, 'LogisticRegression', 'BoW (1-4gram)')\nresults_df = pd.concat([results_df, pd.DataFrame([row])], ignore_index=True)\n\n# Multinomial NB + BoW(1-4)\nbow_13 = CountVectorizer(ngram_range=(1,4), max_features=1_000_000)\nnb = MultinomialNB()\nrow = eval_sklearn(nb, bow_13, X_train_texts, X_val_texts, y_train, y_val, 'MultinomialNB', 'BoW (1-4gram)')\nresults_df = pd.concat([results_df, pd.DataFrame([row])], ignore_index=True)\n\n# Optional: SVM fine-tune (fixed 1-4 grams)\nfrom sklearn.model_selection import GridSearchCV\nfrom sklearn.pipeline import Pipeline\nsvm_pipeline = Pipeline([\n    ('tfidf', TfidfVectorizer(ngram_range=(1,4), max_features=2_000_000, sublinear_tf=True)),\n    ('svm', LinearSVC())\n])\nparam_grid = {\n    'tfidf__min_df': [1,3],\n    'tfidf__max_df': [0.8, 0.9, 1.0],\n    'svm__C': [0.5, 1, 2],\n    'svm__class_weight': [None, 'balanced']\n}\ngrid = GridSearchCV(svm_pipeline, param_grid, scoring='f1_weighted', cv=3, n_jobs=-1, verbose=1)\ngrid.fit(X_train_texts, y_train)\nbest_svm = grid.best_estimator_\nprint('Best params:', grid.best_params_)\nfrom joblib import dump\ndump(best_svm, PATHS['svm_joblib'])\nprint('Saved best SVM ->', PATHS['svm_joblib'])\ny_pred = best_svm.predict(X_val_texts)\nacc = accuracy_score(y_val, y_pred)\nprec, rec, f1, _ = precision_recall_fscore_support(y_val, y_pred, average='weighted', zero_division=0)\nrow = {\n    'model': 'SVM(GridSearchCV)', 'vectorizer': 'TF-IDF (1â€“4gram, tuned)',\n    'accuracy': round(acc,4), 'precision': round(prec,4), 'recall': round(rec,4), 'f1': round(f1,4),\n    'timestamp': time.strftime('%Y-%m-%d %H:%M:%S')\n}\nresults_df = pd.concat([results_df, pd.DataFrame([row])], ignore_index=True)\nplot_confusion(y_val, y_pred, list(le.classes_), 'SVM (tuned)')\n","metadata":{"id":"classic","trusted":true,"execution":{"iopub.status.busy":"2025-10-26T16:04:08.517748Z","iopub.execute_input":"2025-10-26T16:04:08.518040Z","iopub.status.idle":"2025-10-26T16:11:08.446295Z","shell.execute_reply.started":"2025-10-26T16:04:08.518014Z","shell.execute_reply":"2025-10-26T16:11:08.445605Z"}},"outputs":[{"name":"stdout","text":"SVM + TF-IDF (1-4gram): acc=0.7342 f1_w=0.7308\n              precision    recall  f1-score   support\n